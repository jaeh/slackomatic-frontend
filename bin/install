#!/bin/bash
cd /home/pi/nodejs/;

#installs dependencies needed for production
npm install --production
#installs a google webfont for local use and serving
webfont-dl http://fonts.googleapis.com/css?family=Ubuntu+Mono:400,700 \
  --font-out=public/font \
  --out public/css/include/font.styl \
  --woff1=link \
  --woff2=link
